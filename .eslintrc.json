{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    // "ignorePatterns":["node_modules/*"],
    "rules": {
        "no-var":"error",
        "max-len":0,
        "valid-jsdoc":0,
        "no-unused-vars":0
    }
}